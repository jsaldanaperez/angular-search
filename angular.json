    "customers-e2e": {
      "root": "apps/customers-e2e",
      "sourceRoot": "apps/customers-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/customers-e2e/cypress.json",
            "devServerTarget": "customers:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "customers:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/customers-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["customers"]
    },
    "shared-search": {
      "projectType": "library",
      "root": "libs/shared/search",
      "sourceRoot": "libs/shared/search/src",
      "prefix": "angular-search",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "outputs": ["dist/libs/shared/search"],
          "options": {
            "project": "libs/shared/search/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/shared/search/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/shared/search/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/search"],
          "options": {
            "jestConfig": "libs/shared/search/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/search/src/**/*.ts",
              "libs/shared/search/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "shell": {
      "projectType": "application",
      "root": "apps/shell",
      "sourceRoot": "apps/shell/src",
      "prefix": "angular-search",
      "architect": {
        "build": {
